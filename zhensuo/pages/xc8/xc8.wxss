.wrap{
    padding:0rpx 30rpx;
}
.titile{
    font-size: 30rpx;
    color: #00acc0;
    line-height: 80rpx;
    font-weight: bold;
}
.content{
    font-size: 25rpx;
    color: #666;
    line-height: 50rpx;
}
.img>image{
    height: 360rpx;
    border-radius: 10rpx;
    width: 100%;
}
.img{
    display: block;
    width: 100%;
    margin: 0 auto;
    margin-top: 10rpx;
}
.imgs{
    display: flex;
    justify-content: space-between;
    width: 100%;
    margin-top: 10rpx;
}
.imgs>image{
    width: 33%;
    padding-left: 10rpx;
    height: 120rpx;
    border-radius: 10rpx;
}
.erwema{
    width: 40%;
    margin: 100rpx auto 0 auto;
}
.erwema>image{
    width: 100%;
    height: 265rpx;
}
.erwema>text{
    font-size: 25rpx;
    color: #666;
    text-align: center;
    display: block;
    width: 100%;
}