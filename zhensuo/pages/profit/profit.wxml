<!--pages/ profit.wxml-->
<view class='wrap'>
    <view class='list' wx:for="{{profits}}">
        <view class='title'>
            <text class='clinic_name'>{{item.clinicName}}</text>
            <text class='clinic_money'>￥{{item.moneyCurrent}}</text>
        </view>
        <view class='amount'>
            <text>成交金额（元)：</text>
            <text>{{item.moneyAll}}</text>
        </view>
        <view class='row'>
            <view>
                <text>成交比列：</text>
                <text>{{item.persent}}%</text>
            </view>
            <view>
                <text>{{item.createTime}}</text>
               <!-- <text class='hour'>{{item.time}}</text>-->
            </view>
        </view>
    </view>
    <view class='list'>
    <text>暂无收益</text>
    </view>
