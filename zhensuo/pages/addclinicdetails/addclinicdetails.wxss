page{
    background: #f4f4f4
}
.title{
    background: #ff6763;
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    line-height: 80rpx;
}

/* 信息列表 */
.user_wrap{
    margin-top: 30rpx;
    margin-bottom: 150rpx;
}
.list_wrap{
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    background: #fff;
    line-height: 100rpx;
    font-size:30rpx;
    color: #666;
    vertical-align: middle;
    border-bottom: 1px solid #eee;
}
.right{
   padding-right: 30rpx;
}
.left{
    padding-left: 30rpx
}
.images{
    width: 60rpx;
    height: 60rpx;
    padding-top: 20rpx;
    display: block;
    float: left;
}
.left_text{
    padding-left: 30rpx;
}
.list_name{
    margin-top: 30rpx;
}
.user_photo{
    border: none;
}
.photo{
    display: flex;
    display: -webkit-flex;
    justify-content: flex-start;
    background: #fff;
    padding: 30rpx 0;
}
.photo>image{
    width: 250rpx;
    height: 150rpx;
    border-radius: 10px;
    margin-left: 30rpx;
}
.agen{
   position: fixed;
    bottom: 0rpx;
    width: 100%;
    background: #00acc0;
    color: #fff;
    text-align: center;
    border: none;
    outline: none;
    border-radius: 0;
}
.agen_last{
    background: #fff;
    color: #00acc0;
}
.list_mess{
    border-bottom: none
}
.mess{
    font-size: 30rpx;
    background: #fff;
    padding: 30rpx;
    color: #666
}
.doctor{
    margin-top: 30rpx;
    overflow: hidden
}
.doctor_message{
    width: 45%;
    border: 1px solid #eee;
    text-align: center;
    border-radius: 10rpx;
    background: #fff;
}
.head>image{
    width: 100%;
    height: 276rpx;
}
.doctor>view{
    float: left;
    margin-left: 25rpx;
}
.doctor_name{
    color: #00acc0;
}
.docto_text{
    font-size: 26rpx;
    color: #666
}
.doctos_message{
   line-height: 50rpx;
}