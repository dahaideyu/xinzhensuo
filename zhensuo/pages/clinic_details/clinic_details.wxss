.title{
    line-height: 100rpx;
    background: #00acc0;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    border-top: 1px solid #99f0ff;
    padding: 0 30rpx;
}
.clinic_name{
    color: #fff;
    font: bold;
}
.clinic_status{
    margin-left: 30rpx;
    font-size: 30rpx;
    background: #fff;
    color: #00acc0;
    padding:5rpx 15rpx;
    border-radius:30rpx;
    text-align: center
}
.dates{
    color: #fff;
    font-size: 30rpx;
}
/* 列表信息 */
.details_wrap{
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    width: 100%;
    padding: 30rpx 0;
}
.left{
    padding-left: 30rpx;
    font-size: 30rpx;
    color: #666;
    line-height: 55rpx;
}
.right{
    padding-right: 30rpx;
}
.user_name{
   
}
.right>image{
    width: 220rpx;
    height: 140rpx;
    border-radius: 10rpx;
}
.doctor_message{
    width: 45%;
    border: 1px solid #eee;
    text-align: center;
    border-radius: 10rpx;
}
.head>image{
    width: 100%;
    height: 276rpx;
}
.doctor>view{
    float: left;
    margin-left: 25rpx;
}
.doctor_name{
    color: #00acc0;
}
.docto_text{
    font-size: 26rpx;
}
.doctos_message{
   line-height: 50rpx;
}
.add_clinic{
    position: fixed;
    bottom: 0;
    width: 100%;
    border: none;
    border-radius: 0;
    outline: none;
    background: #00acc0;
    color: #fff;
}
.add_class{
    background: #fff;
    color: #00acc0;
}